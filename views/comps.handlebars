<div>
  <div class="jumbotron text-center">
    <h1>Comps</h1>
  </div>
  <div class="homeBlock mt-15 container">
     {{#each rentalComps}}
      <div class="row mt-10 ml-30 mr-30 border border-primary" data-id={{subject}}>
          <div class="col-6">
            <p style="padding-top:2ex">FMLS: {{comp_fmls}}</p>
            <p>Address: {{comp_address}}</p>
          </div>
          <div class="col-6">
            <p style="padding-top:2ex">SQFT: {{comp_size}}</p>
            <p>Bedrooms: {{comp_B}}</p>
            <p>Price/SQFT: {{psf}} </p>
            <p>Build: {{comp_year}}</p>
            <p>List Price: ${{rental}}</p>
          </div>
      </div>
     {{/each}}
      {{#each saleComps}}
      <div class="row mt-10 ml-30 mr-30 border border-primary" data-id={{subject}}>
          <div class="col-6">
            <p style="padding-top:2ex">FMLS: {{comp_fmls}}</p>
            <p>Address: {{comp_address}}</p>
          </div>
          <div class="col-6">
            <p style="padding-top:2ex">SQFT: {{comp_size}}</p>
            <p>Bedrooms: {{comp_B}}</p>
            <p>Price/SQFT: {{psf}} </p>
            <p>Build: {{comp_year}}</p>
            <p>List Price: ${{market_value}}</p>
          </div>
      </div>
     {{/each}}
  </div>
  <!-- Footer -->
  <footer class="page-footer font-small blue">
